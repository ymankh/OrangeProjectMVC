@model IEnumerable<OrangeProjectMVC.Models.Date>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nomination_start_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nomination_end_date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.election_start_date)
        </th>
