        <td>
            @Html.DisplayFor(modelItem => item.nomination_start_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nomination_end_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.election_start_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.election_end_date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.results_date)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
